generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model DeclaracionAduanera {
  id         String    @id @default(uuid())
  desadu     String?
  aduana     String?
  anio       Int?
  serial     String?
  nro_consec String?
  nro_item   Int?
  partida_ar String?
  unid_med   String?
  descripcio String?
  pais_orige String?
  acuerdo_co String?
  regimen    Int?
  p_bruto    Decimal?  @db.Decimal(18, 2)
  p_neto     Decimal?  @db.Decimal(18, 2)
  cantidad   Decimal?  @db.Decimal(18, 2)
  estado_mer String?
  cif_item   Decimal?  @db.Decimal(18, 2)
  flete_item Decimal?  @db.Decimal(18, 2)
  seg_item   Decimal?  @db.Decimal(18, 2)
  gast_item  Decimal?  @db.Decimal(18, 2)
  fob        Decimal?  @db.Decimal(18, 2)
  cif        Decimal?  @db.Decimal(18, 2)
  importador String?
  nit_desp   String?
  despachant String?
  proveedor  String?
  pais_pro   String?
  depto_des  String?
  fecha_reg  DateTime?
  mes        String?
  canal      String?
  tipo_proc  String?
  embarque   String?
  adu_ing    String?
  fecha_reci DateTime?
  datosExtra Json?
  createdAt  DateTime  @default(now())

  @@index([pais_orige])
  @@index([importador])
  @@index([mes])
  @@index([fecha_reg])
  @@index([partida_ar])
  @@index([depto_des])
  @@index([fecha_reci])
}

model PartidaArancelaria {
  codigo      String   @id
  capitulo    String
  descripcion String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@index([capitulo])
}
